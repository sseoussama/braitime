{"components/cloudnav/account-menu":"\n{{!-- icon menu --}}\n<div class=\"icon-menu\">\n\t<ul class=\"inline\">\n\n\t\t{{!--\n\t\t<li class=\"icon-itm a--account\">\n\t\t\t<a href=\"{{urls.account.index}}\" class=\"lv1\">\n\t\t\t\t<i class=\"fa fa-user\" aria-hidden=\"true\"></i>\n\t\t\t</a>\n\t\t\t<div class=\"dropdown-menu fixed\" id=\"cart-preview-dropdown1\" data-dropdown-content aria-hidden=\"true\">\n\t\t\t\t{{> components/cloudnav/cart-preview}}\n\t\t\t</div>\n\t\t</li> --}}\n\t\t\n\t\t{{!-- item CART--}}\n\t\t<li class=\"icon-itm a--account pr\">\n\t\t\t<a\n            class=\"navUser-action2 lv1 dfs\">\n\t\t\t\t<i class=\"fa fa-user\" aria-hidden=\"true\"></i>\n\t\t\t</a>\n\t\t\t<div class=\"account_drop\">\n\t\t\t\t{{> components/cloudnav/account-links}}\n\t\t\t</div>\n\t\t</li>\n\n\n\t\t{{!-- item CART--}}\n\t\t<li class=\"icon-itm a--cart\">\n\t\t\t<a\n            class=\"navUser-action2 lv1\"\n            data-cart-preview\n            data-dropdown=\"cart-preview-dropdown1\"\n            data-options=\"align:right\"\n            href=\"{{urls.cart}}\">\n\t\t\t\t<i class=\"fa fa-shopping-basket\" aria-hidden=\"true\"></i>\n\t\t\t\t{{#if cart.items}}<span class=\"badge qty\">{{cart.quantity}}</span>{{/if}}\n\t\t\t</a>\n\t\t\t<div class=\"dropdown-menu fixed\" id=\"cart-preview-dropdown1\" data-dropdown-content aria-hidden=\"true\">\n\t\t\t\t{{!-- {{> components/cloudnav/cart-preview}} --}}\n\t\t\t</div>\n\t\t</li>\n\n\t</ul>\n</div>\n\n<script>\n\t$('.dfs').click(function(e){\n\t\te.preventDefault();\n\t\tconsole.log('trying');\n\t\tif($('.account_drop').hasClass('is-open')){\n\t\t\t$('.account_drop').removeClass('is-open');\n\t\t}else{\n\t\t\t$('.account_drop').addClass('is-open');\n\t\t}\n\t});\n</script>\n\n\n\n\n\n\n\n\n\n\n\n\n","components/cloudnav/cart-preview":"<div class=\"preview-bg\"></div>\n\n<div class=\"upsell-column animated1 fadeInLeft\">\n    {{> components/cloudnav/cart-upsell}}\n</div>\n\n\n<div class=\"previewCart\">\n    {{> components/cloudnav/cart-meter}}\n    {{#if cart.items.length}}\n        <ul class=\"previewCartList\">\n            <br>\n            <h2 class=\"hide\">Items: <span data-cartQuantity>{{cart.quantity}}</span></h2>\n            {{#each (limit cart.items 10)}}\n                <li class=\"previewCartItem\">\n\n                    <a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\" class=\"previewCartItem-content columns\">\n                        \n                        <!-- image -->\n                        <div href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\" class=\"column previewCartItem-image\">\n                            {{#if type '==' 'GiftCertificate'}}\n                                <img src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\n                            {{else}}\n                                <img src=\"{{getImage image 'productthumb_size' (cdn ../../theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                            {{/if}}\n                        </div>\n\n                        <!-- brand -->\n                        <span class=\"column previewCartItem-brand hide\">\n                            {{brand.name}}\n                        </span>\n                        \n                        \n                        <div class=\"column previewCartItem-price\">\n                            <!-- name -->\n                            <h6 class=\"column1 previewCartItem-name\">\n                                {{!-- <small>{{sku}}</small> --}}\n                                <strong>{{name}}</strong>\n                            </h6>\n                            <!-- price -->\n                            {{#if quantity '>' 1}}\n                                {{quantity}} &times;\n                            {{/if}}\n                            {{#or ../customer (if ../theme_settings.restrict_to_login '!==' true)}}\n                                {{price.formatted}}\n                            {{else}}\n                                {{> components/common/login-for-pricing}}\n                            {{/or}}\n                            \n                            {{#if configurable_fields}}\n                                {{#each (limit configurable_fields 1)}}\n                                    <span class=\"badge green-bg {{name}}\">\n                                        -{{value}}\n                                    </span>\n                                {{/each}}\n                            {{/if}}\n                            <div>\n                            {{#if options}}\n                                {{#each options}}\n                                     <span class=\"{{name}}\">\n                                        <span>{{name}}: </span>\n                                        <strong> {{value}}</strong>\n                                    </span>\n                                {{/each}}\n                            {{/if}}\n                            </div>\n                        </div>\n\n                        <!-- remove -->\n                        <div class=\"column delete_item\" data-cart-itemid=\"{{id}}\"><i class=\"fas fa-trash\"></i></div>\n                        \n\n\n\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n        <div class=\"previewCartAction\">\n            {{> components/cloudnav/cart-mini-totals cart=cart}}\n            <div class=\"disclaim dark\">See discounts on cart page</div>\n            <div class=\"columns is-multiline\">\n                {{#if cart.show_primary_checkout_button}}\n                    <div class=\"column is-12  previewCartAction-checkout\">\n                        <a href=\"{{urls.checkout.single_address}}\" class=\"button button--small button--primary\">\n                            {{lang 'cart.preview.checkout_now'}}\n                        </a>\n                    </div>\n                {{/if}}\n\n                <div class=\"column  is-12 previewCartAction-viewCart{{#unless cart.show_primary_checkout_button}} previewCartAction-viewCart--sole{{/unless}}\">\n                    <a href=\"{{urls.cart}}\" class=\"button button--small button--action\">\n                        {{lang 'cart.preview.view_cart'}}\n                    </a>\n                </div>\n            </div>\n\n            {{#if cart.show_multiple_address_shipping}}\n                <div class=\"column previewCartAction-checkoutMultiple\">\n                    <a href=\"{{urls.checkout.multiple_address}}\">\n                        {{lang 'cart.preview.checkout_multiple'}}\n                    </a>\n                </div>\n            {{/if}}\n        </div>\n    {{else}}\n        <div class=\"previewCart-emptyBody\">\n            {{lang 'cart.checkout.empty_cart'}}\n        </div>\n    {{/if}}\n</div>\n\n\n\n\n\n\n\n\n\n\n\n<script type=\"text/javascript\">\n    // function carts() { \n    //     const appClientId = \"fcrfh0dlg9naige3uj7v4m3uhs2v8up\";\n    //     const xmlhttp = new XMLHttpRequest();\n    //     xmlhttp.onreadystatechange = function() {\n    //         if (xmlhttp.readyState == 4 ) {\n    //            if (xmlhttp.status == 200) {\n    //                console.info('Customer JWT:\\n' + xmlhttp.responseText);\n    //            }else if (xmlhttp.status == 404) {\n    //               console.info('Not logged in!');\n    //            }else {\n    //                console.error('ERROR: ',xmlhttp.status);\n    //            }\n    //         }\n    //     };\n    //     xmlhttp.open(\"GET\", \"/api/carts?include=lineItems.physicalItems.options,lineItems.digitalItems.options\", true);\n    //     xmlhttp.send();\n    // }\n    // carts();\n</script>â€‹\n\n","components/cloudnav/account-links":"<ul class=\"account-links\">\n\n{{!-- account links --}}\n{{#if settings.account_creation_enabled}}\n  {{#unless customer.id}}\n\n\t{{!-- NOT logged in --}}\n    <li><a href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a></li>\n    <li><a href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a></li>\n\n\n\n\t{{!-- logged in --}}\n  {{else}}\n  \t<li class=\"authed\"><h6>{{customer.email}}</h6></li>\n    <li class=\"authed\"><a href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n\t</li>\n\t{{#if settings.returns_enabled}}<li class=\"authed\"><a href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a></li>\n\t{{/if}}\n\t<li class=\"authed\"><a href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\n\t</li>\n\t<li class=\"authed\"><a href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</span></a>\n\t</li>\n\t{{!-- <li class=\"authed\"><a href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n\t</li> --}}\n\t<li class=\"authed\"><a href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n\t</li>\n\t<li class=\"authed\"><a href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n\t</li>\n\t<hr class=\"authed\">\n    <li class=\"authed\"><a href=\"{{urls.auth.logout}}\">Logout</a></li>\n  \t<li class=\"hide welcome\"><a href=\"{{urls.account.inbox}}\">{{customer.email}}</a></li>\n\n\n  {{/unless}}\n{{/if}}\n\n</ul>","components/cloudnav/cart-meter":"<div class=\"row counter\">\n  <div class=\"tac\">\n    <h3>Almost!</h3>\n    <p></p>\n  </div>\n  <div>\n    <div class=\"counter-in-minicart\">\n      <div class=\"meter js-meter\">\n        <div class=\"meter-bar js-meter-bar\"></div>\n        <div class=\"would\" data-tooltip=\"if this were added\" data-placement=\"bottom\"></div>\n        <div class=\"meter-info\"></div>\n        <div class=\"waypoints\"></div> <!-- ticks added in script -->\n      </div>\n      <div class=\"layers\">\n        <div class=\"layer freeShipping-m\" data-tooltip=\"Free Shipping ($25)\" data-placement=\"bottom\">\n          <span class=\"promo\">Free <i class=\"fa fa-truck\"></i></span>\n        </div>\n      </div> <!-- others added in script -->\n    </div>\n  </div>\n</div>\n\n<!-- <div class=\"layer\" data-tooltip=\"$${gate}\" data-placement=\"bottom\"> <span class=\"promo\">${short}</span> </div> -->\n\n<script>\n  $('.js-meter-bar').unbind();\n  var grand_total =\"{{cart.grand_total.formatted}}\".replace('$','');\n  grand_total = grand_total.replace(',', '');\n  var total = parseFloat(grand_total);\n  var cap = 60;\n  var milestones = []\n\n  total = (total>cap) ? cap : total;\n\n  // progress\n  var progress = grand_total/cap;\n  progress = (progress>1) ? .97*100 : progress*100;\n  \n  // freeShipping\n  newTick('freeShipping', 25, 'Shipping', 'Free Shipping');\n  // newTick('tenOff', 100, '10%', '10% Off');\n  // newTick('twentyOff', 200, '20%', '20% Off', 'left');\n\n\n  // set progress\n  $('.js-meter-bar').css('width', progress+'%');\n\n  // Change Messge if goal reached\n  if(progress>96) {\n    $('.tac h3').text('You Qualify for Free Shipping.')\n  }\n\n  function newTick(name, gate, short, long, placement) {\n    placement = (placement) ? placement : 'bottom';\n    var awarded = (gate<=total);\n    var x = parseFloat((gate/cap)*100)+'%';\n    var t = `<div class=\"layer\" style=\"margin-left:${x}\" data-tooltip=\"${long} ($${gate}+)\" data-placement=\"${placement}\"> <span class=\"promo\">${short}</span> </div>`\n    var manual = $(`.${name}-m`);\n   \n    // place waypoint\n    $('.waypoints').append(`<div class=\"waypoint\" style=\"margin-left:${x}\"></div>`);\n    \n    // append layer or just set margin\n    if (manual.length) {\n      $(`.${name}-m`).css('margin-left',x);\n    }else {\n      $('.layers').append(t);\n    }\n\n    // milestones\n    if(!awarded) {\n      milestones.push({\n        fault: gate-total,\n        msg: `You are $${parseFloat(gate-total).toFixed(2)} away from getting ${long}!`\n      });\n    }\n  }\n\n  function SortByFault(a, b){\n    var aa = a.fault;\n    var bb = b.fault; \n    return ((aa < bb) ? -1 : ((aa > bb) ? 1 : 0));\n  }\n  // milestones.sort(SortByName);\n  // milestones.sort(SortByFault)\n  // $('.tac p').html(milestones[0].msg);\n\n</script>\n\n\n\n\n\n\n\n\n\n\n\n\n\n","components/cloudnav/cart-upsell":"\n<h3>Quick Adds</h3>\n\n{{#each products.featured}}\n\t<div class=\"upsell-item animated slideInLeft\">\n\n\t\t{{!-- img --}}\n        <a href=\"{{url}}\" class=\"upsell-img-container\">\n            <img \n            \tclass=\"upsell-image lazyload\" \n            \tdata-sizes=\"auto\" \n            \tsrc=\"{{cdn 'img/loading.svg'}}\" \n            \tdata-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" \n            \talt=\"{{image.alt}}\" \n            \ttitle=\"{{image.alt}}\"\n            >\n        </a>\n\n        {{!-- name --}}\n        <a href=\"{{url}}\" class=\"name\">\n        \t<h6>{{name}}</h6>{{product.options}}\n        </a>\n\n\n        {{!-- add button --}}\n        <div class=\"cloudnav_quick_addtocart_{{id}} quick-add-btn\"></div>\n    </div>\n    {{> components/products/addtocart-cloudnav quick='quickadd'}}\n{{/each}}\n\n\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\n","components/cloudnav/cart-mini-totals":"<hr>\n<ul class=\"cart-totals\">\n    <li>\n        {{lang 'cart.checkout.grand_total'}}:\n        {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n            <span>{{cart.grand_total.formatted}}</span>\n        {{else}}\n            {{> components/common/login-for-pricing}}\n        {{/or}}\n    </li>\n</ul>\n\n\n{{!-- <ul class=\"cart-totals\">\n    <li class=\"cart-total\">\n        <div class=\"cart-total-label\">\n            <strong>{{lang 'cart.checkout.subtotal'}}:</strong>\n        </div>\n        <div class=\"cart-total-value\">\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                <span>{{cart.sub_total.formatted}}</span>\n            {{else}}\n                {{> components/common/login-for-pricing}}\n            {{/or}}\n        </div>\n    </li>\n    {{#if cart.gift_wrapping_cost.amount}}\n        <li class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.gift_wrapping'}}:</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                <span>{{cart.gift_wrapping_cost.formatted}}</span>\n            </div>\n        </li>\n    {{/if}}\n    {{#if cart.shipping_handling.show_estimator}}\n        <li class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.shipping'}}:</strong>\n            </div>\n            {{> components/cart/shipping-estimator cart.shipping_handling}}\n        </li>\n    {{/if}}\n    {{#each cart.taxes}}\n        <li class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{name}}:</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                <span>{{cost.formatted}}</span>\n            </div>\n        </li>\n    {{/each}}\n    {{#if cart.discount }}\n        <li class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.discount'}}:</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                {{cart.discount.formatted}}\n            </div>\n        </li>\n    {{/if}}\n    {{#if cart.coupons}}\n        {{#each cart.coupons}}\n            <li class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\n                    <div>\n                        <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\n                    </div>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{discount.formatted}}</span>\n                </div>\n            </li>\n        {{/each}}\n    {{else}}\n        <li class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.coupon_code'}}:</strong>\n            </div>\n            {{> components/cart/coupon-input}}\n        </li>\n    {{/if}}\n    {{#if settings.gift_certificates_enabled}}\n        {{#each cart.gift_certificates}}\n            <li class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\n                    <span>\n                        {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\n                    </span>\n                </div>\n                <div class=\"cart-total-value\">\n                    {{used.formatted}}\n                </div>\n            </li>\n        {{/each}}\n        <li class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\n            </div>\n            {{> components/cart/gift-certificate-input}}\n        </li>\n    {{/if}}\n    <li class=\"cart-total\">\n        <div class=\"cart-total-label\">\n            <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\n        </div>\n        <div class=\"cart-total-value cart-total-grandTotal\">\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                <span>{{cart.grand_total.formatted}}</span>\n            {{else}}\n                {{> components/common/login-for-pricing}}\n            {{/or}}\n        </div>\n    </li>\n</ul>\n --}}","components/products/addtocart-cloudnav":"\n<a \n    href=\"{{add_to_cart_url}}\" \n    data-cart-item-add-{{quick}}\n    data-cart-item-add-2\n    data-wait-message=\"{{lang 'products.adding_to_cart'}}\"\n    class=\"button hide button--small card-figcaption-button\"\n    data-submits=\"{{id}}\"\n>\n    {{lang 'products.add_to_cart'}}\n    {{#if quick}}\n        <i class=\"fa fa-arrow-right\"></i>\n    {{/if}}\n</a>\n\n\n<form data-item-add-{{id}}  class=\"hide {{quick}}\">\n    <input type=\"text\" name=\"action\" value=\"add\">\n    <input type=\"text\" name=\"product_id\" value=\"{{id}}\"/>\n    <input type=\"text\" name=\"qty[]\" value=\"1\">\n    <input type=\"text\" name=\"url\" value=\"{{url}}\">\n    <input id=\"form-action-addToCart\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary\" type=\"submit\" value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\">\n\n\n    {{!-- still working on accessing options --}}\n    <div data-product-option-change style=\"display:none;\">\n        {{#each options}}\n            <h3>{{this.display_name}}</h3>\n            {{{dynamicComponent 'components/products/options'}}}\n        {{/each}}\n    </div>\n\n\n\n\n\n</form>\n\n{{!-- <div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div> --}}\n\n\n","components/cart/shipping-estimator":"{{#if shipping_cost}}\n    <div class=\"cart-total-value\">\n        <div class=\"subtotal shipping-estimate-show\">\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\n        </div>\n    </div>\n{{else}}\n    <div class=\"cart-total-value\">\n        <button class=\"shipping-estimate-show\">{{lang 'cart.shipping_estimator.add_info'}}</button>\n        <button class=\"shipping-estimate-hide\" style=\"display: none;\">Cancel</button>\n    </div>\n{{/if}}\n\n<div class=\"shipping-estimator u-hiddenVisually\">\n    <form class=\"form estimator-form\" data-shipping-estimator>\n        <dl>\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\n                    {{#each countries}}\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                            {{name}}\n                        </option>\n                    {{/each}}\n                </select>\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                {{#if states.length}}\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\n                        {{#each states}}\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                                {{name}}\n                            </option>\n                        {{/each}}\n                    </select>\n                {{else}}\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\n                {{/if}}\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\n            </dd>\n            <button class=\"button button--primary button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\n        </dl>\n    </form>\n    <div class=\"shipping-quotes\"></div>\n</div>\n","components/cart/coupon-input":"<div class=\"cart-total-value\">\n\n    <button class=\"coupon-code-add\">{{lang 'cart.coupons.add_coupon'}}</button>\n\n    <button class=\"coupon-code-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form coupon-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n","components/cart/gift-certificate-input":"<div class=\"cart-total-value\">\n\n    <button class=\"gift-certificate-add\">{{lang 'cart.gift_certificates.gift_certificate'}}</button>\n\n    <button class=\"gift-certificate-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form gift-certificate-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"certcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.gift_certificates.empty_error'}}\"  id=\"certcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n","components/products/options/date":"<div class=\"form-field\" data-product-attribute=\"date\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        {{this.display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n        {{#if earliest_year '==' latest_year}}\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n        {{/if}}\n    </select>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input \n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"attribute-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}\n            checked\n            data-default\n        {{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"attribute-{{id}}\">{{label}}</label>\n</div>\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-file\" type=\"file\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n\n</div>\n","components/products/options/input-text":"<div class=\"form-field\" data-product-attribute=\"input-text\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n","components/products/options/input-numbers":"<div class=\"form-field\" data-product-attribute=\"input-number\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input\" type=\"number\" id=\"attribute_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n","components/products/options/product-list":"<div class=\"form-field\" data-product-attribute=\"product-list\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}:\n        <span data-option-value></span>\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#if values.0.image}}\n        <ul class=\"productOptions-list\">\n            {{#unless required}}\n                <li class=\"productOptions-list-item\">\n                    <input class=\"form-radio\"\n                           type=\"radio\"\n                           name=\"attribute[{{id}}]\"\n                           value=\"0\"\n                           id=\"attribute_0_{{id}}\"\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\n                    <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\n                </li>\n            {{/unless}}\n            {{#each values}}\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\n                    {{#if image}}\n                        <figure class=\"productOptions-list-item-figure\">\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        </figure>\n                    {{/if}}\n                    <div class=\"productOptions-list-item-content\">\n                        <input\n                            class=\"form-radio\"\n                            type=\"radio\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            id=\"attribute_{{id}}\"\n                            {{#if selected}}\n                                checked\n                                data-default\n                            {{/if}}\n                            {{#if ../required}}required{{/if}}>\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        {{#unless required}}\n        <input class=\"form-radio\"\n               type=\"radio\"\n               name=\"attribute[{{id}}]\"\n               value=\"0\"\n               id=\"attribute_0_{{id}}\"\n               {{#if defaultValue '==' 0}}checked{{/if}}>\n        <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\n        {{/unless}}\n        {{#each values}}\n            <input\n                class=\"form-radio\"\n                type=\"radio\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                id=\"attribute_{{id}}\"\n                {{#if selected}}checked{{/if}}\n                {{#if ../required}}required{{/if}}>\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n","components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n","components/products/options/set-rectangle":"\n{{!-- style for tooltip --}}\n<style type=\"text/css\">\n    [data-product-attribute] .form-option {\n        overflow: visible;\n    }\n</style>\n\n<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        \n\n        {{#if this.label '==' 'Small'}}\n            <label \n                class=\"form-option\" \n                for=\"attribute_{{id}}\" \n                data-product-attribute-value=\"{{id}}\"\n                data-tooltip=\"Fits Men's Shoe Size 5-9. Small feet need great socks too!\"\n                data-placement=\"bottom\"\n            >\n                <span class=\"form-option-variant\">{{this.label}}</span>\n            </label>\n        \n\n\n        {{else if this.label '==' 'Regular'}}\n            <label \n                class=\"form-option\" \n                data-tooltip=\"Fits Men's Shoe Size 9-13. With additional stretch if needed.\"\n                for=\"attribute_{{id}}\" \n                data-product-attribute-value=\"{{id}}\"\n                data-placement=\"bottom\"\n            >\n                    <span class=\"form-option-variant\">{{this.label}}</span>\n            </label>\n        \n\n\n        {{else}}\n            <label class=\"form-option\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\n                    <span class=\"form-option-variant\">{{this.label}}</span>\n            </label>\n        {{/if}}\n    {{/each}}\n</div>\n\n","components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n","components/products/options/swatch":"<div class=\"form-field\" data-product-attribute=\"swatch\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}\" {{#if selected}}checked data-default{{/if}} {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-swatch\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            {{#if image}}\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\n            {{/if}}\n            {{#if data.[2]}}\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\n            {{else}}\n                {{#if data.[1]}}\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                {{else}}\n                    {{#if data.[0]}}\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n            {{#if pattern}}\n                <span class=\"form-option-expanded\">\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n                </span>\n            {{/if}}\n        </label>\n    {{/each}}\n</div>\n","components/products/options/textarea":"<div class=\"form-field\" data-product-attribute=\"textarea\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <textarea class=\"form-input\" id=\"attribute_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>\n        {{prefill}}\n    </textarea>\n</div>\n"}
