{"components/cloudnav/home-admin":"<style type=\"text/css\">\n    @media (min-width: 801px){\n        .body {\n            margin-top: 2.5rem; \n        }\n    }\n</style>\n\n<!-- pre-compiled upload template -->\n<div class=\"prevv hide\">\n    <div class=\"dz-preview dz-file-preview\">\n        <img data-dz-thumbnail />\n        <div class=\"confirm-upload\">\n            <div class=\"dz-details\">\n                <div class=\"dz-filename\"><span data-dz-name></span></div>\n                <div class=\"dz-size\" data-dz-size></div>\n            </div>\n            <a class=\"button db save\">publish</a>\n            <a class=\"button db cancel\" data-dz-remove>Cancel</a>\n            <div class=\"dz-progress\">\n                <span class=\"dz-upload\" data-dz-uploadprogress></span>\n            </div>\n            <!-- <div class=\"dz-success-mark\"><span>✔sdfsdf</span></div> -->\n            <!-- <div class=\"dz-error-mark\"><span>adfasdfsdf✘</span></div> -->\n            <div class=\"dz-error-message\"><span data-dz-errormessage></span></div>\n        </div>\n    </div>\n</div>\n<!-- contextmenu -->\n<ul class=\"contextMenu\" id=\"context_dz1\" hidden>\n    <form id=\"contextForm\">\n        <div class=\"info mono\">\n            <p class=\"z_id\">zone_id: <span class=\"zone_id\"></span></p>\n            <p class=\"dims\"></p>\n        </div>\n        <input type=\"hidden\" name=\"zone\" class=\"zone disabled\">\n        <label class=\"mono\">image url</label>\n        <input type=\"text\" name=\"image\" class=\"image\">\n        <label class=\"mono\">link path</label>\n        <input type=\"text\" name=\"path\" class=\"path\">\n        <label class=\"mono\">SEO Text</label>\n        <input type=\"text\" name=\"alt\" class=\"alt\">\n        <br>\n        <input type=\"submit\" value=\"publish\" class=\"button green-bg white\">\n    </form>\n</ul>"}
