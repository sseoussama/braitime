{"components/products/tabs":"<ul class=\"tabs\" data-tab role=\"tablist\">\n    {{#if product.related_products}}\n        <li class=\"tab is-active\" role=\"presentational\">\n            <a class=\"tab-title\" href=\"#tab-related\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-related\">{{lang 'products.related_products'}}</a>\n        </li>\n    {{/if}}\n    {{#if product.similar_by_views}}\n        <li class=\"tab{{#unless product.related_products}} is-active{{/unless}}\" role=\"presentational\">\n            <a class=\"tab-title\" href=\"#tab-similar\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" controls=\"tab-similar\">{{lang 'products.similar_by_views'}}</a>\n        </li>\n    {{/if}}\n</ul>\n\n<div class=\"tabs-contents\">\n{{#if product.related_products}}\n    <div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent is-active\" id=\"tab-related\">\n        {{> components/products/carousel products=product.related_products columns=6}}\n    </div>\n{{/if}}\n\n{{#if product.similar_by_views}}\n    <div role=\"tabpanel\" aria-hidden=\"true\" class=\"tab-content has-jsContent{{#unless product.related_products}} is-active{{/unless}}\" id=\"tab-similar\">\n        {{> components/products/carousel products=product.similar_by_views columns=6}}\n    </div>\n{{/if}}\n</div>\n","components/products/carousel":"<section class=\"productCarousel\"\n    data-slick='{\n        \"dots\": true,\n        \"infinite\": false,\n        \"mobileFirst\": true,\n        \"slidesToShow\": 2,\n        \"slidesToScroll\": 2,\n        \"touchThreshold\": 10,\n        \"responsive\": [\n            {\n                \"breakpoint\": 1260,\n                \"settings\": {\n                    \"slidesToScroll\": 3,\n                    \"slidesToShow\": 5.4\n                }\n            },\n            {\n                \"breakpoint\": 800,\n                \"settings\": {\n                    \"slidesToScroll\": 3,\n                    \"slidesToShow\": 5\n                }\n            },\n            {\n                \"breakpoint\": 550,\n                \"settings\": {\n                    \"slidesToScroll\": 3,\n                    \"slidesToShow\": 3\n                }\n            }\n        ]\n    }'\n>\n    {{#each products}}\n    <div class=\"productCarousel-slide\">\n        {{> components/products/card theme_settings=../theme_settings customer=../customer}}\n    </div>\n    {{/each}}\n</section>\n","components/products/card":"\n<article class=\"card {{#if alternate}}card--alternate{{/if}}\">\n    <a href=\"https://store-xba1p2i5uz.mybigcommerce.com/manage/products/{{id}}/edit\" target=\"_blank\" class=\"edit\"><i class=\"fas fa-pencil-alt\"></i></a>\n\n\n\n    {{#if grooms}}\n        <div class=\"grooms-overlay\" data-name=\"{{name}}\" data-pid=\"{{id}}\"></div>\n    {{/if}}\n\n\n\n    <figure class=\"card-figure {{#if grooms}}disable{{/if}}\">\n        {{#or price.non_sale_price_with_tax price.non_sale_price_without_tax}}\n            {{#if theme_settings.product_sale_badges '===' 'sash'}}\n                <div class=\"sale-flag-sash\">\n                    <span class=\"sale-text\">On Sale!</span>\n                </div>\n            {{else if theme_settings.product_sale_badges '===' 'topleft'}}\n                <div class=\"sale-flag-side\">\n                    <span class=\"sale-text\">On Sale!</span>\n                </div>\n            {{else if theme_settings.product_sale_badges '===' 'burst'}}\n                <div class=\"starwrap\">\n                    <div class=\"sale-text-burst\">On Sale!</div>\n                    <div class=\"sale-flag-star\"></div>\n                </div>\n            {{/if}}\n        {{/or}}\n        {{#if demo}}\n            <div class=\"card-img-container\">\n                <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n            </div>\n        {{else}}\n            <a href=\"{{url}}\">\n                <div class=\"card-img-container\">\n                    <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                    {{!-- <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn7.bigcommerce.com/s-xba1p2i5uz/product_images/uploaded_images/kelkl7ya.png?t=1533568323&_ga=2.232273247.1536004184.1533501411-1453691217.1526140694\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\"> --}}\n                </div>\n            </a>\n        {{/if}}\n        <figcaption class=\"card-figcaption\">\n            <div class=\"card-figcaption-body\">\n                {{#unless hide_product_quick_view}}\n                    {{#if theme_settings.show_product_quick_view}}\n                        {{#unless demo}}\n                            <a href=\"#\" class=\"button button--small card-figcaption-button quickview\" data-product-id=\"{{id}}\">{{lang 'products.quick_view'}}</a>\n                        {{/unless}}\n                    {{/if}}\n                {{/unless}}\n                {{!-- {{#if show_compare}}\n                    <label class=\"button button--small card-figcaption-button\" for=\"compare-{{id}}\">\n                        {{lang 'products.compare'}} <input type=\"checkbox\" name=\"products[]\" value=\"{{id}}\" id=\"compare-{{id}}\" data-compare-id=\"{{id}}\">\n                    </label>\n                {{/if}} --}}\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                    {{#if show_cart_action}}\n                        {{#if has_options}}\n                            {{!-- <a href=\"{{url}}\" class=\"button button--small card-figcaption-button\" data-product-id=\"{{id}}\">{{lang 'products.choose_options'}}</a> --}}\n                        {{/if}}\n                        {{#if pre_order}}\n                            <a href=\"{{pre_order_add_to_cart_url}}\" class=\"button button--small card-figcaption-button\">{{lang 'products.pre_order'}}</a>\n                        {{/if}}\n                        {{!-- {{#if add_to_cart_url }} --}}\n                            <div class=\"cloudnav_addtocart_{{id}}\"></div>\n                            {{!-- <a href=\"{{add_to_cart_url}}\" class=\"button button--small card-figcaption-button\">{{lang 'products.add_to_cart'}}</a> --}}\n                        {{!-- {{/if}} --}}\n                        {{#if out_of_stock_message }}\n                            <a href=\"{{url}}\" class=\"button button--small card-figcaption-button\" data-product-id=\"{{id}}\">{{out_of_stock_message}}</a>\n                        {{/if}}\n                    {{/if}}\n                {{/or}}\n                {{#if add_to_cart_url }}\n                {{/if}}\n            </div>\n        </figcaption>\n    </figure>\n    <div class=\"card-body {{#if grooms}}disable{{/if}}\">\n            <div class=\"bg-light\">\n                {{> components/products/addtocart-cloudnav}}\n            </div>\n        {{#if show_rating}}\n            <p class=\"card-text\" data-test-info-type=\"productRating\">\n                <span class=\"rating--small\">\n                    {{> components/products/ratings rating=rating}}\n                </span>\n            </p>\n        {{/if}}\n        {{#if brand.name}}\n            <p class=\"card-text brand\" data-test-info-type=\"brandName\">{{brand.name}}</p>\n        {{/if}}\n        <h4 class=\"card-title\">\n            {{#if demo}}\n                {{name}}\n            {{else}}\n                <div class=\"f price yellow\"><strong>{{> components/products/price price=price}}</strong></div>\n                <a href=\"{{url}}\">{{name}}</a>\n            {{/if}}\n        </h4>\n\n        <div class=\"card-text\" data-test-info-type=\"price\">\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                {{> components/products/price price=price}}\n            {{else}}\n                {{> components/common/login-for-pricing}}\n            {{/or}}\n        </div>\n        {{> components/products/bulk-discount-rates}}\n    </div>\n</article>\n","components/products/addtocart-cloudnav":"\n<a \n    href=\"{{add_to_cart_url}}\" \n    data-cart-item-add-{{quick}}\n    data-cart-item-add-2\n    data-wait-message=\"{{lang 'products.adding_to_cart'}}\"\n    class=\"button hide button--small card-figcaption-button\"\n    data-submits=\"{{id}}\"\n>\n    {{lang 'products.add_to_cart'}}\n    {{#if quick}}\n        <i class=\"fa fa-arrow-right\"></i>\n    {{/if}}\n</a>\n\n\n<form data-item-add-{{id}}  class=\"hide {{quick}}\">\n    <input type=\"text\" name=\"action\" value=\"add\">\n    <input type=\"text\" name=\"product_id\" value=\"{{id}}\"/>\n    <input type=\"text\" name=\"qty[]\" value=\"1\">\n    <input type=\"text\" name=\"url\" value=\"{{url}}\">\n    <input id=\"form-action-addToCart\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary\" type=\"submit\" value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\">\n\n\n    {{!-- still working on accessing options --}}\n    <div data-product-option-change style=\"display:none;\">\n        {{#each options}}\n            <h3>{{this.display_name}}</h3>\n            {{{dynamicComponent 'components/products/options'}}}\n        {{/each}}\n    </div>\n\n\n\n\n\n</form>\n\n{{!-- <div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div> --}}\n\n\n","components/products/ratings":"{{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n        <span class=\"icon icon--ratingFull\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{else}}\n        <span class=\"icon icon--ratingEmpty\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{/if}}\n{{/for}}\n{{{snippet 'product_rating'}}}\n","components/products/price":"{{#and price.price_range (if theme_settings.price_ranges '==' true)}}\n    {{> components/products/price-range price_range=price.price_range schema_org=schema_org}}\n{{else}}\n    {{#if price.with_tax}}\n        <div class=\"price-section price-section--withTax rrp-price--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\n            {{#if price.rrp_with_tax}}\n                {{lang 'products.retail_price'}}\n                <span data-product-rrp-with-tax class=\"price price--rrp\"> {{price.rrp_with_tax.formatted}}</span>\n            {{/if}}\n        </div>\n        <div class=\"price-section price-section--withTax non-sale-price---withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\n            {{#if price.non_sale_price_with_tax}}\n                {{lang 'products.price_was'}}\n                <span data-product-non-sale-price-with-tax class=\"price price--rrp\"> {{price.non_sale_price_with_tax.formatted}}</span>\n            {{/if}}\n        </div>\n        <div>\n            <span class=\"price-now-label\">\n                {{#if price.non_sale_price_with_tax}}\n                    {{lang 'products.price_now'}}\n                {{/if}}\n            </span>\n            <span data-product-price-with-tax class=\"price\"> {{price.with_tax.formatted}}</span>\n            {{#if schema_org}}\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\n                <div itemprop=\"priceSpecification\" itemscope itemtype=\"http://schema.org/PriceSpecification\">\n                    <meta itemprop=\"price\" content=\"{{price.with_tax.value}}\">\n                    <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\n                    <meta itemprop=\"valueAddedTaxIncluded\" content=\"true\">\n                </div>\n            {{/if}}\n            {{#if price.without_tax}}\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_with_tax' tax_label=price.tax_label}}</abbr>\n            {{/if}}\n        </div>\n    {{/if}}\n    {{#if price.without_tax}}\n        <div class=\"price-section rrp-price--withoutTax price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\n            {{#if price.rrp_without_tax}}\n                {{!-- {{lang 'products.retail_price'}} --}}\n                {{!-- <span data-product-rrp-price-without-tax class=\"price price--rrp\"> {{price.rrp_without_tax.formatted}}</span> --}}\n            {{/if}}\n        </div>\n        <div class=\"price-section non-sale-price---withoutTax price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\n            {{#if price.non_sale_price_without_tax}}\n                {{lang 'products.price_was'}}\n                <span data-product-non-sale-price-without-tax class=\"price_ price--rrp\"> {{price.non_sale_price_without_tax.formatted}}</span>\n            {{/if}}\n        </div>\n        <div>\n            <span class=\"price-now-label\">\n                {{#if price.non_sale_price_without_tax}}\n                    {{!-- {{lang 'products.price_now'}} --}}\n                {{/if}}\n            </span>\n            <span data-product-price-without-tax class=\"price price--withoutTax1\"> {{price.without_tax.formatted}}</span>\n            {{#if schema_org}}\n                <meta itemprop=\"availability\" itemtype=\"http://schema.org/ItemAvailability\"\n                    content=\"http://schema.org/{{#if product.pre_order}}PreOrder{{else if product.out_of_stock}}OutOfStock{{else if product.can_purchase '===' false}}OutOfStock{{else}}InStock{{/if}}\">\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\n                <div itemprop=\"priceSpecification\" itemscope itemtype=\"http://schema.org/PriceSpecification\">\n                    <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\n                    <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\n                    <meta itemprop=\"valueAddedTaxIncluded\" content=\"false\">\n                </div>\n            {{/if}}\n            {{#if price.with_tax}}\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\n            {{/if}}\n        </div>\n    {{/if}}\n    {{!-- {{#if page_type '===' 'product'}}\n        {{#if price.saved}}\n             <div class=\"price-section price-section--saving price\">\n                    <span class=\"price\">{{lang 'products.you_save_opening_text'}}</span>\n                    <span data-product-price-saved class=\"price\">\n                        {{price.saved.formatted}}\n                    </span>\n                    <span class=\"price\">{{lang 'products.you_save_closing_bracket'}}</span>\n             </div>\n        {{/if}}\n    {{/if}} --}}\n{{/and}}\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\n","components/products/bulk-discount-rates":"\n{{#if product.bulk_discount_rates.length}}\n    {{!-- {{#if product}}<dt class=\"productView-info-name\">{{lang 'products.bulk_pricing.title'}}</dt>{{/if}} --}}\n    <dd class=\"productView-info-value\">\n        <a href=\"{{url}}\"\n           {{#unless is_ajax }}data-reveal-id=\"bulkPricingModal{{#if id}}-{{id}}-{{$index}}{{/if}}\">{{/unless}}\n            {{lang 'products.bulk_pricing.view'}}\n        </a>\n    </dd>\n    <div id=\"bulkPricingModal{{#if id}}-{{id}}-{{$index}}{{/if}}\" class=\"modal modal--small\" data-reveal>\n        <div class=\"modal-header\">\n            <h2 class=\"modal-header-title\">{{lang 'products.bulk_pricing.modal_title'}}</h2>\n            <a href=\"#\" class=\"modal-close\" aria-label=\"Close\"><span aria-hidden=\"true\">&#215;</span></a>\n        </div>\n        <div class=\"modal-body\">\n            <p>{{lang 'products.bulk_pricing.instructions'}}</p>\n            <br>\n            <ul>\n                {{#each product.bulk_discount_rates}}\n                <li>\n                    {{lang 'products.bulk_pricing.range' min=min max=max}}\n                    {{#if type '===' 'percent'}}\n                    {{lang 'products.bulk_pricing.percent' discount=discount.formatted}}\n                    {{/if}}\n                    {{#if type '===' 'fixed'}}\n                    {{lang 'products.bulk_pricing.fixed' discount=discount.formatted}}\n                    {{/if}}\n                    {{#if type '===' 'price'}}\n                    {{lang 'products.bulk_pricing.price' discount=discount.formatted}}\n                    {{/if}}\n                </li>\n                {{/each}}\n            </ul>\n            <p class=\"disclaim\">\n                <em>**Product must be same style and flavor to qualify for bulk pricing.</em>\n            </p>\n        </div>\n    </div>\n{{/if}}\n","components/products/price-range":"{{#and price_range.min.with_tax price_range.max.with_tax}}\n    <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\n        <span data-product-price-with-tax class=\"price\">{{price_range.min.with_tax.formatted}} - {{price_range.max.with_tax.formatted}}</span>\n        {{#and price_range.min.without_tax price_range.max.without_tax}}\n            <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_with_tax' tax_label=price_range.min.tax_label}}</abbr>\n        {{else if schema_org}}\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\n                <div itemprop=\"priceSpecification\" itemscope itemtype=\"http://schema.org/PriceSpecification\">\n                    <meta itemprop=\"minPrice\" content=\"{{price_range.min.with_tax.value}}\"  />\n                    <meta itemprop=\"price\" content=\"{{price_range.min.with_tax.value}}\">\n                    <meta itemprop=\"maxPrice\" content=\"{{price_range.max.with_tax.value}}\"  />\n                    <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\n                    <meta itemprop=\"valueAddedTaxIncluded\" content=\"true\">\n                </div>\n        {{/and}}\n    </div>\n{{/and}}\n{{#and price_range.min.without_tax price_range.max.without_tax}}\n    <div class=\"price-section price-section--withoutTax {{#and price_range.min.with_tax price_range.max.with_tax}}price-section--minor{{/and}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\n        <span data-product-price-without-tax class=\"price price--withoutTax\">{{price_range.min.without_tax.formatted}} - {{price_range.max.without_tax.formatted}}</span>\n        {{#and price_range.min.with_tax price_range.max.with_tax}}\n            <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price_range.min.tax_label}}</abbr>\n        {{/and}}\n        {{#if schema_org}}\n            <meta itemprop=\"availability\" content=\"{{product.availability}}\">\n            <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\n            <div itemprop=\"priceSpecification\" itemscope itemtype=\"http://schema.org/PriceSpecification\">\n                <meta itemprop=\"minPrice\" content=\"{{price_range.min.without_tax.value}}\"  />\n                <meta itemprop=\"price\" content=\"{{price_range.min.without_tax.value}}\">\n                <meta itemprop=\"maxPrice\" content=\"{{price_range.max.without_tax.value}}\"  />\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\n                <meta itemprop=\"valueAddedTaxIncluded\" content=\"false\">\n            </div>\n        {{/if}}\n    </div>\n{{/and}}\n","components/products/options/date":"<div class=\"form-field\" data-product-attribute=\"date\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        {{this.display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n        {{#if earliest_year '==' latest_year}}\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n        {{/if}}\n    </select>\n</div>\n","components/products/options/input-numbers":"<div class=\"form-field\" data-product-attribute=\"input-number\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input\" type=\"number\" id=\"attribute_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input \n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"attribute-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}\n            checked\n            data-default\n        {{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"attribute-{{id}}\">{{label}}</label>\n</div>\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-file\" type=\"file\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n\n</div>\n","components/products/options/input-text":"<div class=\"form-field\" data-product-attribute=\"input-text\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n","components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n","components/products/options/product-list":"<div class=\"form-field\" data-product-attribute=\"product-list\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}:\n        <span data-option-value></span>\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#if values.0.image}}\n        <ul class=\"productOptions-list\">\n            {{#unless required}}\n                <li class=\"productOptions-list-item\">\n                    <input class=\"form-radio\"\n                           type=\"radio\"\n                           name=\"attribute[{{id}}]\"\n                           value=\"0\"\n                           id=\"attribute_0_{{id}}\"\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\n                    <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\n                </li>\n            {{/unless}}\n            {{#each values}}\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\n                    {{#if image}}\n                        <figure class=\"productOptions-list-item-figure\">\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        </figure>\n                    {{/if}}\n                    <div class=\"productOptions-list-item-content\">\n                        <input\n                            class=\"form-radio\"\n                            type=\"radio\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            id=\"attribute_{{id}}\"\n                            {{#if selected}}\n                                checked\n                                data-default\n                            {{/if}}\n                            {{#if ../required}}required{{/if}}>\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        {{#unless required}}\n        <input class=\"form-radio\"\n               type=\"radio\"\n               name=\"attribute[{{id}}]\"\n               value=\"0\"\n               id=\"attribute_0_{{id}}\"\n               {{#if defaultValue '==' 0}}checked{{/if}}>\n        <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\n        {{/unless}}\n        {{#each values}}\n            <input\n                class=\"form-radio\"\n                type=\"radio\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                id=\"attribute_{{id}}\"\n                {{#if selected}}checked{{/if}}\n                {{#if ../required}}required{{/if}}>\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n","components/products/options/set-rectangle":"\n{{!-- style for tooltip --}}\n<style type=\"text/css\">\n    [data-product-attribute] .form-option {\n        overflow: visible;\n    }\n</style>\n\n<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        \n\n        {{#if this.label '==' 'Small'}}\n            <label \n                class=\"form-option\" \n                for=\"attribute_{{id}}\" \n                data-product-attribute-value=\"{{id}}\"\n                data-tooltip=\"Fits Men's Shoe Size 5-9. Small feet need great socks too!\"\n                data-placement=\"bottom\"\n            >\n                <span class=\"form-option-variant\">{{this.label}}</span>\n            </label>\n        \n\n\n        {{else if this.label '==' 'Regular'}}\n            <label \n                class=\"form-option\" \n                data-tooltip=\"Fits Men's Shoe Size 9-13. With additional stretch if needed.\"\n                for=\"attribute_{{id}}\" \n                data-product-attribute-value=\"{{id}}\"\n                data-placement=\"bottom\"\n            >\n                    <span class=\"form-option-variant\">{{this.label}}</span>\n            </label>\n        \n\n\n        {{else}}\n            <label class=\"form-option\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\n                    <span class=\"form-option-variant\">{{this.label}}</span>\n            </label>\n        {{/if}}\n    {{/each}}\n</div>\n\n","components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n","components/products/options/swatch":"<div class=\"form-field\" data-product-attribute=\"swatch\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}\" {{#if selected}}checked data-default{{/if}} {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-swatch\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            {{#if image}}\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\n            {{/if}}\n            {{#if data.[2]}}\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\n            {{else}}\n                {{#if data.[1]}}\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                {{else}}\n                    {{#if data.[0]}}\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n            {{#if pattern}}\n                <span class=\"form-option-expanded\">\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n                </span>\n            {{/if}}\n        </label>\n    {{/each}}\n</div>\n","components/products/options/textarea":"<div class=\"form-field\" data-product-attribute=\"textarea\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <textarea class=\"form-input\" id=\"attribute_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>\n        {{prefill}}\n    </textarea>\n</div>\n"}
