{"components/custom/faq":"<!-- onclick=\"renderItems(\\{{:slug}})\" -->\n<script id=\"tabs_markup\" type=\"text/x-jsrender\">\n  <li>\n      <a href=\"#\\{{:slug}}\" data-slug=\"\\{{:slug}}\" aria-controls=\"\\{{:slug}}\" role=\"tab\" data-toggle=\"tab\">\n        <span>\\{{:name}}</span>\n      </a>\n  </li>\n</script>\n\n<script id=\"category_markup\" type=\"text/x-jsrender\">\n  <div role=\"tabpanel\" class=\"tab-pane\" id=\"\\{{:slug}}\">\n    <div class=\"container\">\n      <h2 class=\"heading-primary\">\\{{:name}} Questions</h2>\n      <div class=\"accordion\">\n        <dl>\n          <div data-questions-\\{{:slug}}></div>\n        </dl>\n      </div>\n    </div>\n  </div>\n</script>\n\n<script id=\"questions_markup\" type=\"text/x-jsrender\">\n  \\{{if q}}\n  <dt>\n    <a href=\"#accordion21\" data-id=\"\\{{:id}}\" data-in=\"\\{{:in}}\" aria-expanded=\"false\" aria-controls=\"accordion21\" class=\"accordion-title accordionTitle js-accordionTrigger\">\\{{:q}}</a>\n  </dt>\n  <dd class=\"accordion-content accordionItem is-collapsed\" id=\"accordion21\" aria-hidden=\"true\">\n    <p>\\{{:a}}</p>\n  </dd>\n  \\{{/if}}\n</script>\n\n<script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js\" integrity=\"sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa\" crossorigin=\"anonymous\"></script>\n\n{{!-- {{{stylesheet '/assets/css/faq.css'}}} --}}\n\n\n\n\n\n\n\n\n\n\n\n\n<div id=\"faq\">\n  <div id=\"info_tabs\" class=\"content-containe1r\">\n\n    <div class=\"dark-bg\">\n      <!-- head -->\n      <div class=\"info-header text-center\">\n        <div class=\"container1\">\n          <h1>What's your trouble bubble?</h1>\n          <p class=\"sub-title\">This is a short list of our most frequently asked questions.</p>\n\n          {{> components/cloudnav/faq-add-form}}\n        </div>\n      </div>\n\n      <!-- tabs -->\n      <ul class=\"nav nav-tabs m0\" role=\"tablist\" data-tabs>\n      </ul>\n    </div>\n\n    <!-- categories -->\n    <div class=\"tab-content\" style=\"display: block;\" data-categories>\n    </div>\n\n  </div>\n</div>\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n","components/cloudnav/faq-add-form":"<!-- contextmenu -->\n<ul class=\"contextMenu\" id=\"context_dz1\" hidden>\n    <form id=\"contextForm\">\n\n        <!-- select category -->\n\t\t<p class=\"form-g\">\n        \t<label class=\"mono\">Category</label>\n\t\t\t<select name=\"category\" id=\"categoryS\" data-categories-select>\n\t\t\t\t<option value=\"null\">Select a Category</option>\t\n\t\t\t\t<option value=\"new\">new*</option>\n\t\t\t</select>\n\t\t</p>\n\n\t\t<!-- new category -->\n\t\t<p class=\"p-new hide form-g\">\n\t\t\t<label class=\"mono\">New Category Name</label>\n\t\t\t<input type=\"text\" id=\"category\" name=\"category\" required>\n\t\t</p>\n\n\t\t<!-- Question -->\n\t\t<p class=\"form-g\">\n\t\t\t<label class=\"mono\">Question</label>\n\t\t\t<input type=\"text\" id=\"question\" name=\"question\" required>\n\t\t</p>\n\n\t\t<!-- answer -->\n\t\t<p class=\"form-g\">\n\t\t\t<label class=\"mono\">answer</label>\n\t\t\t<input type=\"text\" id=\"answer\" name=\"answer\" required>\n\t\t</p>\n\n\t\t<!-- submit -->\n        <hr>\n        <input type=\"submit\" value=\"publish\" class=\"button green-bg white\">\n    \t<a class=\"red mono hide\">remove question</a>\n\n    </form>\n\n</ul>\n\n\n\n<script id=\"category_select\" type=\"text/x-jsrender\">\n\t<option class=\"added\" value=\"\\{{:slug}}\">\\{{:name}}</option>\t\n</script>"}
